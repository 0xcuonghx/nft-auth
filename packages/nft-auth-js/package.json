  "module": "dist/nft-auth-js.esm.js",
  "size-limit": [
    {
      "path": "dist/nft-auth-js.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/nft-auth-js.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.2",
    "@types/jsonwebtoken": "^9.0.6",
    "husky": "^9.0.11",
    "size-limit": "^11.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@cuonghx.gu-tech/nft-auth-contracts": "*",
    "ethers": "^6.11.1",
    "jsonwebtoken": "^9.0.2"
